{
  "version": 2,
  "builds": [
    {
      "src": "./app.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/url/(.*)",
      "dest": "/url/$1"
    },
    {
      "src": "/user/(.*)",
      "dest": "/user/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
